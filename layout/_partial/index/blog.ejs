<div id="blog" class="row container section scrollspy">
  <h3>My Blog</h3>
  <div class="col s9">
    <div class="slider">
      <ul class="slides">
        <% for(var i = 0; i < theme.recent_blog_count; i++){ %>  
          <li>
          <img src="Blog Image">
          <div class="caption center-align">
            <h3>Blog Title</h3>
            <h5 class="light grey-text text-lighten-3">Blog Time</h5>
          </div>
          </li>
        <% } %>
      </ul>
    </div>
  </div>
  <div class="col s3">
    <h5 class="header">Tag</h5>
    <p class="grey-text text-darken-3 lighten-3"><%= theme.introduction %></p>
    <p><a class="waves-effect waves-light btn">button</a></p>
  </div>
</div>